{
    "name": "d4h/finediff",
    "description": "PHP implementation of a Fine granularity Diff engine",
    "license": "MIT",
    "keywords": ["finediff", "diff", "text", "string", "opcode"],
    "authors": [
        {
            "name": "Raymond Hill"
        },
        {
            "name": "Rob Crowe"
        },
        {
            "name": "Hannes Van De Vreken",
            "email": "vandevreken.hannes@gmail.com"
        },
        {
            "name": "Igor Finagin",
            "email": "igor@finag.in"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.7",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpunit/phpunit": "^8.5 || ^9.2",
        "spaze/phpstan-disallowed-calls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "FineDiff\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FineDiff\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
