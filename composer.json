{
    "name": "dh4/finediff",
    "description": "PHP implementation of a Fine granularity Diff engine",
    "license": "MIT",
    "keywords": ["finediff", "diff", "text", "string", "opcode"],
    "authors": [
        {
            "name": "Raymond Hill"
        },
        {
            "name": "Rob Crowe"
        },
        {
            "name": "Hannes Van De Vreken",
            "email": "vandevreken.hannes@gmail.com"
        },
        {
            "name": "Igor Finagin",
            "email": "igor@finag.in"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "FineDiff\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FineDiff\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
